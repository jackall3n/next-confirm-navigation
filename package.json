{
  "name": "next-confirm-navigation",
  "version": "0.0.0",
  "description": "A library for confirming navigation in Next.js",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/jackall3n/next-confirm-navigation#readme",
  "workspaces": [
    "demo"
  ],
  "bugs": {
    "url": "https://github.com/jackall3n/next-confirm-navigation/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackall3n/next-confirm-navigation.git"
  },
  "author": "Jack Allen <gh@jck.ai>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "bumpp": "^10.1.0",
    "happy-dom": "^17.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.11.1",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "next": "^15.5.2"
  }
}
